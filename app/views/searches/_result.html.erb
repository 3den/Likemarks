<div class="result row">
  <div class="span12">
    <h3><%= link_to result.name, result.link, targed: "_blank" %></h3>
    <%= image_tag(result.picture, class: "link-picture") %>
    <p class="link-users">
      <small>by</small>
      <%= raw link_to_users(result.users) %>
    </p>
    <p class="span10"><small><%= time_ago_in_words result.created_time %></small>
      <%= result.message %></p>
  </div>
</div>
